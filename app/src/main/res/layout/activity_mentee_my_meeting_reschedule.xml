<?xml version="1.0" encoding="utf-8"?>
<layout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		>

	<data>

		<variable
				name="vm"
				type="com.tsic.ui.screen.mentee_bottom_menu.mymeeting.requested.reschedule.MenteeMyMeetingRescheduleViewModel"
				/>

		<variable
				name="activity"
				type="com.tsic.ui.screen.mentee_bottom_menu.mymeeting.requested.reschedule.MenteeMyMeetingRescheduleActivity"
				/>
	</data>

	<androidx.coordinatorlayout.widget.CoordinatorLayout
			android:id="@+id/root_layout"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			tools:context=".ui.MyMeetingActivity"
			>

		<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				>
			<com.google.android.material.appbar.AppBarLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@color/colorToolbarTranslucentGreen"
					android:theme="@style/AppTheme.AppBarOverlay.Local"
					>

				<androidx.appcompat.widget.Toolbar
						android:id="@+id/toolbar"
						android:layout_width="match_parent"
						android:layout_height="?android:attr/actionBarSize"
						android:background="@color/colorToolbarGreen"
						app:popupTheme="@style/AppTheme.PopupOverlay"
						/>

			</com.google.android.material.appbar.AppBarLayout>



			<include
					android:id="@+id/content_layout"
					layout="@layout/content_mentor_my_meeting_reschedule"
					app:viewModel="@{vm}"
					/>

		</LinearLayout>

	</androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>
