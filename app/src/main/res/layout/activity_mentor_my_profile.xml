<?xml version="1.0" encoding="utf-8"?>
<layout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		>

	<data>

		<variable
				name="vm"
				type="com.tsic.ui.screen.mentor_bottom_menu.myprofile.MentorMyProfileViewModel"
				/>

		<variable
				name="activity"
				type="com.tsic.ui.screen.mentor_bottom_menu.myprofile.MentorMyProfileActivity"
				/>
	</data>

	<LinearLayout
			android:id="@+id/root_layout"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			tools:context=".ui.screen.mentor_bottom_menu.myprofile.MentorMyProfileActivity"
			>

		<include
				android:id="@+id/content_layout"
				layout="@layout/content_mentor_my_profile"
				app:activity="@{activity}"
				app:viewModel="@{vm}"
				/>

	</LinearLayout>
</layout>
