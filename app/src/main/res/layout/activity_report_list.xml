<?xml version="1.0" encoding="utf-8"?>
<layout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		>


	<data>

		<variable
				name="vm"
				type="com.tsic.ui.screen.mentor_drawer_menu.report_list.MentorReportViewModel"
				/>

		<variable
				name="activity"
				type="com.tsic.ui.screen.mentor_drawer_menu.report_list.ReportListActivity"
				/>
	</data>


	<androidx.coordinatorlayout.widget.CoordinatorLayout
			android:id="@+id/root_layout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			tools:context=".ui.MyUploadsActivity"
			>

		<com.google.android.material.appbar.AppBarLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@null"
				android:theme="@style/AppTheme.AppBarOverlay.Local"
				>

			<androidx.appcompat.widget.Toolbar
					android:id="@+id/toolbar"
					android:layout_width="match_parent"
					android:layout_height="?android:attr/actionBarSize"
					android:background="@color/colorToolbarGreen"
					app:popupTheme="@style/AppTheme.PopupOverlay"
					app:title="@string/app_name"
					/>

		</com.google.android.material.appbar.AppBarLayout>


		<include
				android:id="@+id/content_layout"
				layout="@layout/content_mentor_reports"
				app:viewModel="@{vm}"
				/>

		<!--

-->

	</androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>